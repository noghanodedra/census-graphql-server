  "devDependencies": {
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.11",
